                            <input type="password" name="password" id="password" style="width:100%; padding:2%;"><br><br><br>
                            <button class="btn btn-lg bg-color-theme" style="border-radius:0%; color:white; width:49%;"
                                type="submit">REGISTER</button>
                            <a href="/login"><div class="btn btn-lg btn-light" style="border-radius:0%; width:49%;background-color: rgb(200, 200, 200);"type="submit">LOGIN</div></a><br><br><br>
